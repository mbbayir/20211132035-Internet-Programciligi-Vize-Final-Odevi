
@{
    ViewData["Title"] = "GetUserList";
    Layout = "~/Views/Shared/_LayoutAdminLTE.cshtml";
}

@model List<UserModel>

<h1 class="mt-5 mb-4">Kullanıcılar</h1>
<div class="table-responsive">
    <table class="table table-hover">
        <thead class="thead-dark">
            <tr>
                <th>ID</th>
                <th>Adı Soyadı</th>
                <th>Kullanıcı Adı</th>
                <th>E-Posta</th>
                <th>Şehir</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.FullName</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                    <td>@item.City</td>
                </tr>
            }
        </tbody>
    </table>
</div>
